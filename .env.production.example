# SevenThree Production Configuration
# Copy this file to .env.production and fill in your values

# Discord bot token (required)
SEVENTHREE_BOT_TOKEN=your_discord_bot_token_here

# Command prefix (deprecated - slash commands now used)
SEVENTHREE_Prefix=+

# PostgreSQL connection string
# For Docker compose: use 'postgres' as host
# For external database: use your database host
SEVENTHREE_ConnectionStrings__SevenThree=Host=postgres;Port=5432;Database=seventhree;Username=seventhree;Password=CHANGE_THIS_PASSWORD

# Database provider
SEVENTHREE_Database__Provider=Postgres

# QRZ API credentials (optional, for callsign lookups)
SEVENTHREE_QRZ_Username=your_qrz_username
SEVENTHREE_QRZ_Password=your_qrz_password
SEVENTHREE_QRZ_ApiUrl=https://xmldata.qrz.com/xml/current/

# Optional: Log level (debug, info, error)
# SEVENTHREE_LogLevel=info

# PostgreSQL password for docker-compose (used by postgres container)
POSTGRES_PASSWORD=CHANGE_THIS_PASSWORD
